<Window x:Class="WipifiDock.ProjectsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WipifiDock"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        Title="ProjectsWindow"
        Height="360" Width="520" MinWidth="520" MinHeight="360"
        WindowStartupLocation="CenterScreen" Icon="Resources/icon1.png">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="SplashScreen.png" Opacity="0.1" Stretch="UniformToFill"/>
        </Grid.Background>
        <Frame x:Name="frame" Navigating="frame_Navigating"></Frame>
    </Grid>
</Window>
